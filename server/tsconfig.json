{
  "compilerOptions": {
    "lib": [ "es2018" ],
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2018",
    "typeRoots" : ["./node_modules/@types", "./@types"]
  }
}
